            <div class="row wrapper border-bottom white-bg page-heading">
                <div class="col-lg-10">
                    <h2>接觸者資料</h2>
                    <ol class="breadcrumb">
                        <li>
                            事件管理
                        </li>
                        <li>
                            <a href="/view/one_case">個案事件</a>
                        </li>
                        <li>
                            <a href="/view/view_case?disease=measles"> 接觸者名冊建立</a>
                        </li>
                        <li class="active">
                            <strong>接觸者資料</strong>
                        </li>
                    </ol>
                </div>
                <div class="col-lg-2">

                </div>
            </div>
            <div class="wrapper wrapper-content animated fadeInRight">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="ibox-content">
                            <form method="get" class="form-horizontal">
                                <div class="form-group">
                                    <label class="col-sm-2 control-label">序號</label>
                                    <div class="col-sm-10"><input type="text" class="form-control" value="9527"></div>
                                    <label class="col-sm-2 control-label"></label>
                                    <div class="col-sm-10"><p class="form-control-static">9527(NIIS)</p></div>
                                </div>
                                <div class="hr-line-dashed"></div>
                                <div class="form-group">
                                    <label class="col-sm-2 control-label">姓名</label>
                                    <div class="col-sm-10">
                                        <input type="text" class="form-control" value="張國洲">
                                    </div>
                                    <label class="col-sm-2 control-label"></label>
                                    <div class="col-sm-10"><p class="form-control-static">張國洲(NIIS)</p></div>
                                </div>
                                <div class="form-group">
                                    <label class="col-sm-2 control-label">居住地</label>
                                    <div class="col-sm-10">
                                        <input type="text" class="form-control" value="台北">
                                    </div>
                                    <label class="col-sm-2 control-label"></label>
                                    <div class="col-sm-10"><p class="form-control-static">台北(戶役政)</p></div>
                                </div>
                                <div class="form-group">
                                    <label class="col-sm-2 control-label">接觸地點</label>
                                    <div class="col-sm-10">
                                        <input type="text" class="form-control" value="台北">
                                    </div>
                                    <label class="col-sm-2 control-label"></label>
                                    <div class="col-sm-10"><p class="form-control-static">台北(戶役政)</p></div>
                                </div>
                                <div class="form-group">
                                    <label class="col-sm-2 control-label">接觸日期</label>
                                    <div class="col-sm-10">
                                        <input type="text" class="form-control" value="2017/06/05">
                                    </div>
                                    <label class="col-sm-2 control-label"></label>
                                    <div class="col-sm-10"><p class="form-control-static">2017/06/05(NIIS)</p></div>
                                </div>




                                <div class="hr-line-dashed"></div>
                                <div class="form-group">
                                    <label class="col-sm-2 control-label">分類</label>

                                    <div class="col-sm-10"><input class="form-control" name="password" value="麻疹"></div>
                                </div>
                                <div class="hr-line-dashed"></div>
                                <div class="form-group">
                                    <label class="col-sm-2 control-label">出生日期</label>

                                    <div class="col-sm-10"><input type="text" class="form-control" value="80/02/22"></div>
                                </div>
                                <div class="form-group">
                                    <label class="col-sm-2 control-label">性別</label>

                                    <div class="col-sm-10"><input type="text" class="form-control" value="男"></div>
                                </div>
                                <div class="form-group">
                                    <label class="col-sm-2 control-label">疾管署中心</label>
                                    <div class="col-sm-2"><p class="form-control-static">北疾管署制中心</p></div>
                                </div>
                                <div class="form-group">
                                    <label class="col-sm-2 control-label">
                                        是否出國 <br />
                                    </label>

                                    <div class="col-sm-10">
                                        <div data-toggle="buttons" class="btn-group">
                                                    <label class="btn btn-sm btn-white"> <input type="radio" id="option1" name="options"> 是 </label>
                                                    <label class="btn btn-sm btn-white active"> <input type="radio" id="option2" name="options"> 否 </label>                                            
                                                </div>
                                    </div>
                                </div>
                                <div class="hr-line-dashed"></div>
                                <div class="form-group">
                                    <label class="col-sm-2 control-label">是否得過以下疾病</label>

                                    <div class="col-sm-10">
                                        <div class="i-checks"><label> <input type="checkbox" value=""> <i></i> 德國麻疹</label></div>
                                        <div class="i-checks"><label> <input type="checkbox" value=""> <i></i> 麻疹</label></div>
                                    </div>
                                </div>
                                <div class="hr-line-dashed"></div>
                                <div class="form-group">
                                    <label class="col-sm-2 control-label">有無接種疫苗</label>
                                    <div class="col-sm-10">
                                        <label class="checkbox-inline i-checks"> <input type="checkbox" value="option1"></label>
                                    </div>
                                </div>
                                <div class="hr-line-dashed"></div>
                                <div class="form-group has-error">
                                    <label class="col-sm-2 control-label">特別註明</label>

                                    <div class="col-sm-10"><input type="text" class="form-control"></div>
                                </div>
                                <div class="hr-line-dashed"></div>
                                <div class="form-group">
                                    <div class="col-sm-4 col-sm-offset-2">
                                        <button class="btn btn-white" type="submit">取消</button>
                                        <button class="btn btn-primary" type="submit">儲存</button>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
                </div>
                <div class="footer">
                </div>

            </div>
    </div>


    <!-- Mainly scripts -->
    <script src="../javascripts/jquery-3.1.1.min.js"></script>
    <script src="../javascripts/bootstrap.min.js"></script>
